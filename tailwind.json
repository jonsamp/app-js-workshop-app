{
	"static": {
		"style": {
			"position": "static"
		}
	},
	"fixed": {
		"style": {
			"position": "fixed"
		}
	},
	"absolute": {
		"style": {
			"position": "absolute"
		}
	},
	"relative": {
		"style": {
			"position": "relative"
		}
	},
	"block": {
		"style": {
			"display": "block"
		}
	},
	"inline": {
		"style": {
			"display": "inline"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"bg-screen": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(248 249 250 / var(--tw-bg-opacity))"
		}
	},
	"uppercase": {
		"style": {
			"textTransform": "uppercase"
		}
	},
	"capitalize": {
		"style": {
			"textTransform": "capitalize"
		}
	},
	"italic": {
		"style": {
			"fontStyle": "italic"
		}
	},
	"text-blue-500": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(3 102 214 / var(--tw-text-opacity))"
		}
	},
	"dark:bg-screen-dark": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(11 15 19 / var(--tw-bg-opacity))"
		},
		"media": "(prefers-color-scheme: dark)"
	},
	"dark:text-blue-dark-500": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(71 148 253 / var(--tw-text-opacity))"
		},
		"media": "(prefers-color-scheme: dark)"
	}
}