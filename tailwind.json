{
	"static": {
		"style": {
			"position": "static"
		}
	},
	"fixed": {
		"style": {
			"position": "fixed"
		}
	},
	"absolute": {
		"style": {
			"position": "absolute"
		}
	},
	"relative": {
		"style": {
			"position": "relative"
		}
	},
	"block": {
		"style": {
			"display": "block"
		}
	},
	"inline": {
		"style": {
			"display": "inline"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"rounded-lg": {
		"style": {
			"borderTopLeftRadius": 8,
			"borderTopRightRadius": 8,
			"borderBottomRightRadius": 8,
			"borderBottomLeftRadius": 8
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"bg-screen": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(248 249 250 / var(--tw-bg-opacity))"
		}
	},
	"p-6": {
		"style": {
			"paddingTop": 24,
			"paddingRight": 24,
			"paddingBottom": 24,
			"paddingLeft": 24
		}
	},
	"uppercase": {
		"style": {
			"textTransform": "uppercase"
		}
	},
	"capitalize": {
		"style": {
			"textTransform": "capitalize"
		}
	},
	"italic": {
		"style": {
			"fontStyle": "italic"
		}
	},
	"dark:bg-screen-dark": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(11 15 19 / var(--tw-bg-opacity))"
		},
		"media": "(prefers-color-scheme: dark)"
	}
}